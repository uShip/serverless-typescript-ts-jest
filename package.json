{
    "name": "uship-geospatial",
    "version": "0.1.0",
    "description": "Serverless webpack example using Typescript",
    "main": "",
    "scripts": {
        "build:debug": "tsc -p ./config/tsconfig.debug.json --pretty",
        "test": "jest --colors --detectOpenHandles --coverage",
        "test:watch": "jest --watch",
        "lint": "tsc -noemit && eslint . --fix --ext .ts,.tsx",
        "debug": "SET SLS_DEBUG=* && node --inspect node_modules\\serverless\\bin\\serverless offline -s dev"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm run test"
        }
    },
    "dependencies": {
        "request-promise-native": "^1.0.7"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.17",
        "@types/jest": "^24.0.11",
        "@types/node": "^11.11.8",
        "@types/request-promise-native": "^1.0.16",
        "@typescript-eslint/eslint-plugin": "^1.7.0",
        "eslint": "^5.15.3",
        "eslint-config-prettier": "^4.2.0",
        "eslint-plugin-prettier": "^3.0.1",
        "husky": "^1.3.1",
        "jest": "^24.5.0",
        "jest-jenkins-reporter": "^1.0.2",
        "prettier": "^1.17.0",
        "rimraf": "^2.6.3",
        "serverless": "^1.39.1",
        "serverless-offline": "^4.9.2",
        "serverless-webpack": "^5.2.0",
        "source-map-support": "^0.5.6",
        "ts-jest": "^24.0.0",
        "ts-loader": "^5.3.3",
        "typescript": "^3.3.4000",
        "webpack": "^4.5.0"
    },
    "author": "",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/uShip/uShip.Geospatial"
    },
    "homepage": "https://github.com/uShip/uShip.Geospatial#readme",
    "jestSonar": {
        "reportPath": "reports",
        "reportFile": "report.xml",
        "indent": 4
    }
}
